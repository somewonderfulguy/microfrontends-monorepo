    "react-app", "eslint:recommended", "plugin:react/recommended", "plugin:jest/style", "plugin:jest/recommended", "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": ["warn", {"allow": ["warn", "error"]}],
    "@typescript-eslint/explicit-module-boundary-types": "off"
